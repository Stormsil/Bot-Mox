{
  "name": "@botmox/api-contract",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "dist/index.js",
  "types": "src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "src",
    "dist"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "contract:check": "pnpm run typecheck",
    "check": "pnpm run contract:check"
  },
  "dependencies": {
    "@botmox/shared-types": "workspace:*",
    "@ts-rest/core": "^3.52.1",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "typescript": "^5.7.0"
  }
}
