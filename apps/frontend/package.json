{
  "name": "@botmox/frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "predev": "pnpm --filter @botmox/api-contract build",
    "dev": "vite",
    "prebuild": "pnpm --filter @botmox/api-contract build",
    "typecheck": "tsc -b --noEmit",
    "build": "tsc -b && vite build",
    "lint": "pnpm --dir ../.. exec biome check apps/frontend/src && pnpm --dir ../.. run check:ui:boundaries",
    "preview": "vite preview",
    "test:e2e": "playwright test",
    "test:e2e:report": "playwright show-report"
  },
  "dependencies": {
    "@ant-design/icons": "^5.6.1",
    "@botmox/api-contract": "workspace:*",
    "@monaco-editor/react": "^4.7.0",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/context-zone": "^2.5.1",
    "@opentelemetry/exporter-trace-otlp-http": "^0.212.0",
    "@opentelemetry/instrumentation": "^0.212.0",
    "@opentelemetry/instrumentation-document-load": "^0.56.0",
    "@opentelemetry/instrumentation-fetch": "^0.212.0",
    "@opentelemetry/instrumentation-user-interaction": "^0.55.0",
    "@opentelemetry/resources": "^2.5.1",
    "@opentelemetry/sdk-trace-base": "^2.5.1",
    "@opentelemetry/sdk-trace-web": "^2.5.1",
    "@opentelemetry/semantic-conventions": "^1.39.0",
    "@refinedev/antd": "^6.0.3",
    "@refinedev/core": "^5.0.8",
    "@supabase/supabase-js": "^2.95.3",
    "@tanstack/react-query": "^5.90.5",
    "@uiw/react-markdown-preview": "5.1.5",
    "@uiw/react-md-editor": "^4.0.11",
    "antd": "^5.29.3",
    "dayjs": "^1.11.13",
    "monaco-editor": "^0.55.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^7.9.6",
    "recharts": "^3.7.0",
    "remark-gfm": "^4.0.1",
    "zone.js": "^0.16.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@playwright/test": "^1.58.2",
    "@types/node": "^24.10.1",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@vitejs/plugin-react": "^5.1.1",
    "eslint": "^9.39.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^7.2.4"
  }
}
